<div class="frame">
  <header class="header">
    <button class="back-btn" onclick="history.back()">←</button>
    <div class="search-bar">
      <input type="text" id="search" placeholder="Search">
    </div>
    <div class="user-icon">👤</div>
  </header>

  <h2>Super Clubs Borrowing Records</h2>
  <div class="club-list">
    <% @super_clubs.each do |club| %>
      <div class="club-card">
        <p><strong><%= club.Club_Name %></strong></p>
        <%= link_to "→", borrowing_balance_sheet_path(club.id), class: "btn-view-more" %>
      </div>
    <% end %>
  </div>
</div>
