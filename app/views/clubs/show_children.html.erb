<h1>Child Clubs of <%= @parent_club.Club_Name %></h1>
<table>
    <thead>
        <tr>
            <th>Club ID</th>
            <th>Club Name</th>
            <th>Budget</th>
        </tr>
    </thead>
    <tbody>
        <% @child_clubs.each do |club| %>
        <tr onclick="window.location='<%= edit_club_path(club) %>'" style="cursor: pointer;">
            <td><%= club.Club_ID %></td>
            <td><%= club.Club_Name %></td>
            <td><%= club.Budget %></td>
        </tr>
        <% end %>
    </tbody>
</table>